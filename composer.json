{
    "name": "helper-network/omnipay-common",
    "type": "library",
    "description": "Common components for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-common",
    "license": "MIT",
    "authors": [
        {
            "name": "Griffin Meyer",
            "homepage": "https://www.theeventhelper.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-common/contributors"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Common\\" : "src/Common" },
        "classmap": ["src/Omnipay.php"]
    },
    "require": {
        "php-http/client-implementation": "^1",
        "php-http/message": "^1.5",
        "php-http/discovery": "^1.2.1",
        "symfony/http-foundation": "^5",
        "moneyphp/money": "^3.1",
        "helper-network/omnipay-tests": "dev-odm as v4.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "php-http/mock-client": "^1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "suggest": {
        "league/omnipay": "The default Omnipay package provides a default HTTP Adapter."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
